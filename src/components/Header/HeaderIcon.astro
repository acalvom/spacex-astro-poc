---
interface Props {
  paths: string[]
}

const { paths } = Astro.props
---

<svg
  class="w-6 h-6 lg:w-8 lg:h-8"
  viewBox="0 0 24 24"
  stroke-width="1.5"
  stroke="currentColor"
  fill="none"
  stroke-linecap="round"
  stroke-linejoin="round"
>
  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
  {paths.map((path) => <path d={path} />)}
</svg>
